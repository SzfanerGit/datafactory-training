{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "MoviesInput",
						"type": "DatasetReference"
					},
					"name": "InputMovies"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "MoviesOutput",
						"type": "DatasetReference"
					},
					"name": "OutputMovies"
				}
			],
			"transformations": [
				{
					"name": "YearExtrection"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movieId as string,",
				"          title as string,",
				"          genres as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> InputMovies",
				"InputMovies derive(year = toInteger(trim(right(title,6),'()')),",
				"          title = toString(left(title,length(title)-6))) ~> YearExtrection",
				"YearExtrection sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['MoviesAfterPipeline'],",
				"     truncate: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> OutputMovies"
			]
		}
	}
}